{
    "name": "toro/cms-bundle",
    "type": "symfony-bundle",
    "description": "APIs management for Symfony2 projects.",
    "keywords": ["cms"],
    "homepage": "https://github.com/intbizth/toro-cms-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "liverbool",
            "homepage": "https://github.com/liverbool"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",

        "knplabs/knp-menu-bundle": "~1.1",
        "white-october/pagerfanta-bundle": "~1.0",
        "aptoma/twig-markdown": "^2.0",
        "michelf/php-markdown": "^1.6",
        "suncat/mobile-detect-bundle": "1.0.*",
        "symfony/form": "^2.8",
        "symfony/validator": "^2.8"
    },
    "require-dev": {
        "doctrine/orm": "^2.4",
        "phpspec/phpspec": "^3.1",
        "phpunit/phpunit": "^5.6",
        "twig/twig": "^1.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Toro\\Bundle\\CmsBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Toro\\Bundle\\CmsBundle\\spec\\": "spec/" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
